<div class="row">
    <div class="col-12 m-auto">
        <% if(messages.error) {%> 
            <div class="alert alert-danger">
                <%= messages.error %> 
            </div>
        <% } %> 
        <% if(messages.success) {%> 
            <div class="alert alert-success">
                <%= messages.success %> 
            </div>
        <% } %> 
    </div>
</div>